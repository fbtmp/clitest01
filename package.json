{
  "name": "@fbtmp/clitest01",
  "version": "1.0.0",
  "description": "oclif example Hello World CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin/",
    "dist/",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "bin": {
    "clitest01": "./bin/run"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "echo 'version'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbtmp/clitest01.git"
  },
  "keywords": [
    "oclif"
  ],
  "author": "Frieder Bluemle <frieder.bluemle@gmail.com> (https://www.fbluemle.org/)",
  "license": "MIT",
  "bugs": "https://github.com/fbtmp/clitest01/issues",
  "homepage": "https://github.com/fbtmp/clitest01",
  "dependencies": {
    "@console-cat/sdk": "^0.19.0",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.7.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "clitest01",
    "dirname": "clitest01",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
